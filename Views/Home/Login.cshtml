@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>SB Admin 2 - Login</title>

    <!-- Custom fonts for this template-->    <script src="~/js/sb-admin-2.min.js"></script>

    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">

  
    <!-- Custom styles for this template-->
     <link href="~/css/login.css" rel="stylesheet">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">

 
</head>

<body>

    <div class="container">

        <div class="login-header text-center">
            <h2 class="h4 text-gray-900 mb-4">Turn your ideas into action</h2>
            <p class="h4 text-gray-900 mb-4">Log in to your account</p>
        </div>

        <div class="social-login">
            <button class="btn btn-google btn-user btn-block" aria-label="Continue with Google">
                <div class="google-icon">
                    <svg role="graphics-symbol" viewBox="0 0 25 25" class="googleLogo" >
                        <g><path d="M19.9996 10.2297C19.9996 9.54995 19.9434 8.8665 19.8234 8.19775H10.2002V12.0486H15.711C15.4823 13.2905 14.7475 14.3892 13.6716 15.0873V17.586H16.9593C18.89 15.8443 19.9996 13.2722 19.9996 10.2297Z" fill="#4285F4"></path><path d="M10.2002 20.0003C12.9518 20.0003 15.2723 19.1147 16.963 17.5862L13.6753 15.0875C12.7606 15.6975 11.5797 16.0429 10.2039 16.0429C7.54224 16.0429 5.28544 14.2828 4.4757 11.9165H1.08301V14.4923C2.81497 17.8691 6.34261 20.0003 10.2002 20.0003Z" fill="#34A853"></path><path d="M4.47227 11.9163C4.04491 10.6743 4.04491 9.32947 4.47227 8.0875V5.51172H1.08333C-0.363715 8.33737 -0.363715 11.6664 1.08333 14.4921L4.47227 11.9163Z" fill="#FBBC04"></path><path d="M10.2002 3.95756C11.6547 3.93552 13.0605 4.47198 14.1139 5.45674L17.0268 2.60169C15.1824 0.904099 12.7344 -0.0292099 10.2002 0.000185607C6.34261 0.000185607 2.81497 2.13136 1.08301 5.51185L4.47195 8.08764C5.27795 5.71762 7.53849 3.95756 10.2002 3.95756Z" fill="#EA4335"></path></g></svg>
                </div>Continue with Google
            </button>

          
            <button class="btn btn-facebook btn-user btn-block" aria-label="Continue with Facebook">
                <div class="facebook-icon">
                    <svg width="1em" data-e2e="" height="1em" viewBox="0 0 48 48" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M24 47C36.7025 47 47 36.7025 47 24C47 11.2975 36.7025 1 24 1C11.2975 1 1 11.2975 1 24C1 36.7025 11.2975 47 24 47Z" fill="white"></path><path d="M24 1C11.2964 1 1 11.2964 1 24C1 35.4775 9.40298 44.9804 20.3846 46.7205L20.3936 30.6629H14.5151V24.009H20.3936C20.3936 24.009 20.3665 20.2223 20.3936 18.5363C20.4206 16.8503 20.7542 15.2274 21.6288 13.7487C22.9722 11.4586 25.0639 10.3407 27.6335 10.0251C29.7432 9.76362 31.826 10.0521 33.9087 10.3407C34.0529 10.3587 34.125 10.3767 34.2693 10.4038C34.2693 10.4038 34.2783 10.6472 34.2693 10.8005C34.2603 12.4053 34.2693 16.0839 34.2693 16.0839C33.2685 16.0659 31.6096 15.9667 30.5096 16.138C28.6884 16.4175 27.6425 17.5806 27.6064 19.4108C27.5704 20.8354 27.5884 24.009 27.5884 24.009H33.9988L32.962 30.6629H27.5974V46.7205C38.597 44.9984 47.009 35.4775 47.009 24C47 11.2964 36.7036 1 24 1Z" fill="#0075FA"></path></svg> Continue with Facebook
                </div>
        </button>

            <button class="btn btn-user btn-block" aria-label="Continue with LinkedIn">
                <div class="linkedin-icon">
                <img class="XNo5Ab" src="data:image/png;base64," style="height:18px;width:18px" alt="" data-csiid="1nUzZ9S9Fdil2roPkPHk2Ao_3" data-atf="1">
                Continue with LinkedIn
                </div>
            </button>
        </div>
        <form action="/login" method="POST">
            @Html.AntiForgeryToken()
            <div class="form-group">
                <label for="emailOrUsername" class="label-small">Email or Username</label>
                <input type="text" id="emailOrUsername" name="emailOrUsername" class="form-control" placeholder="Email address or username" aria-describedby="emailHelp" autocomplete="username" required />
                <label for="password" class="label-small">Password</label>
                <input type="password" id="password" name="password" class="form-control" placeholder="Password" autocomplete="current-password" required />
                
            
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary btn-block">Continue with password</button>
            </div>
            <a href="/forgot-password" class="fg text-muted mt-2 d-block">Forgot your password?</a>
            <a href="/Home/Register" class="small text-muted mt-2 d-block">Sign up for Mute</a>
        </form>

    </div>

</body>

</html>